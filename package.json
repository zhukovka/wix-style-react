{
  "name": "wix-style-react",
  "description": "wix-style-react",
  "version": "4.18.0",
  "main": "./dist/src/index.js",
  "files": [
    "dist",
    "src",
    "scripts",
    ".babelrc",
    "testkit",
    "new-icons",
    "*.scss",
    "*.js"
  ],
  "bin": {
    "migrate-to-icons-v2": "./scripts/migrate-to-icons-v2/runner.js"
  },
  "scripts": {
    "start": "./scripts/npm-start.sh",
    "pretest": "npm run lint && ./scripts/ensure-dist.sh",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "yoshi test --jest",
    "test:watch": "yoshi test --jest --watch",
    "pretest:e2e": "./scripts/ensure-dist.sh",
    "test:e2e": "concurrently 'npm run test:e2e-only' 'node scripts/e2e-server.js' --names 'protractor,e2e-server' --success first --kill-others",
    "test:e2e-only": "NODE_ENV=production yoshi test --protractor",
    "copy-public-styles": "cp src/mixins.scss mixins.scss && cp src/colors.scss colors.scss",
    "build": "npm run lint && svg2react-icon && npm run proxy-wix-ui-icons && npm run copy-public-styles && yoshi build && build-storybook && npm run export-components",
    "pr-postbuild": "npx teamcity-surge-autorelease@^1.0.0 --dist=storybook-static",
    "storybook": "start-storybook -p 6006",
    "postpublish": "npx teamcity-surge-autorelease@^1.0.0 --dist=storybook-static",
    "export-components": "import-path --path src",
    "lint": "yoshi lint",
    "proxy-wix-ui-icons": "node scripts/proxy-wix-ui-icons"
  },
  "yoshi": {
    "features": {
      "addStoriesToBaseGlob": true
    },
    "entry": {
      "app": "index.js"
    },
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM",
      "Wix": "Wix"
    },
    "externalUnprocessedModules": [
      "get-own-enumerable-property-symbols",
      "stringify-object"
    ],
    "exports": "wix-style-react"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/wix-style-react.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix/wix-style-react/issues"
  },
  "homepage": "https://github.com/wix/wix-style-react#readme",
  "devDependencies": {
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addon-options": "4.0.0-alpha.14",
    "@storybook/react": "4.0.0-alpha.14",
    "@types/react": "^15.5.4",
    "autosuggest-highlight": "^3.1.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "*",
    "babel-preset-stage-2": "*",
    "concurrently": "^3.4.0",
    "copy-to-clipboard": "^3.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.15.2",
    "eyes.it": "^2.0.0",
    "gh-pages-auto-release": "^1.1.2",
    "github-markdown-css": "^2.4.1",
    "highlight.js": "^9.8.0",
    "identity-obj-proxy": "^3.0.0",
    "import-path": "^1.0.0",
    "react": "^15.5.4",
    "react-docgen": "^2.15.0",
    "react-dom": "^15.5.4",
    "react-element-to-jsx-string": "^13.0.0",
    "react-test-renderer": "^15.5.4",
    "react-test-utils": "^0.0.1",
    "regenerator-runtime": "^0.11.0",
    "sinon": "^1.17.6",
    "svg2react-icon": "^1.0.0",
    "wait-for-cond": "^1.0.3",
    "wix-storybook-utils": "^2.0.0",
    "yoshi": "^2.8.3"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "logLevel": "verbose"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "color": "^2.0.0",
    "create-react-context": "^0.2.2",
    "css-element-queries": "^1.0.2",
    "date-fns": "^1.29.0",
    "deep-eql": "^3.0.0",
    "difference": "^1.0.2",
    "hoist-non-react-methods": "^1.1.0",
    "is-image": "^1.0.1",
    "is-url": "^1.2.2",
    "jscodeshift": "^0.5.0",
    "lodash": "^4.17.4",
    "moment": "2.19.2",
    "omit": "^1.0.1",
    "popper.js": "^1.10.8",
    "prop-types": "^15.5.8",
    "rc-slider": "^8.0.2",
    "react-autosuggest": "^7.0.1",
    "react-collapse": "^4.0.3",
    "react-day-picker": "^7.0.7",
    "react-jss": "^8.0.0",
    "react-modal": "^3.3.2",
    "react-motion": "^0.4.7",
    "react-remarkable": "^1.1.1",
    "react-transition-group": "^2.2.1",
    "slate": "^0.20.3",
    "wix-animations": "^1.0.141",
    "wix-eventually": "^2.2.0",
    "wix-ui-backoffice": "1.0.293",
    "wix-ui-icons-common": "2.0.6",
    "wix-ui-test-utils": "^1.0.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/node_modules/babel-polyfill",
      "<rootDir>/test/jest-setup.js"
    ],
    "setupTestFrameworkScriptFile": "wix-ui-test-utils/jest-setup",
    "moduleNameMapper": {
      "\\.(?!\\st).(css|less|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "wix/react",
    "env": {
      "jest": true
    },
    "rules": {
      "react/jsx-handler-names": 0,
      "react/no-find-dom-node": 0,
      "react/no-string-refs": 0,
      "react/jsx-boolean-value": 2,
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-restricted-properties": [
        "error",
        {
          "object": "browser",
          "property": "pause",
          "message": "Do not commit browser.pause()."
        },
        {
          "object": "browser",
          "property": "sleep",
          "message": "Do not commit browser.sleep()."
        },
        {
          "object": "eyes",
          "property": "fit",
          "message": "Do not commit eyes.fit()."
        }
      ],
      "no-restricted-globals": [
        "error",
        "fdescribe",
        "fit"
      ],
      "indent": 0,
      "indent-legacy": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ]
    }
  }
}
