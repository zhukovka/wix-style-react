{
  "name": "wix-style-react",
  "description": "wix-style-react",
  "version": "1.1.0",
  "main": "./dist/src/index.js",
  "files": [
    "dist",
    "src",
    ".babelrc",
    "testkit"
  ],
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "wix-node-build test --jest",
    "test:e2e": "wix-node-build test --protractor",
    "build": "wix-node-build lint && node ./src/Icons/build && wix-node-build build && build-storybook",
    "start": "wix-node-build start & npm run storybook",
    "release": "wix-node-build release && gh-pages-auto-release --dist=storybook-static",
    "storybook": "start-storybook -p 6006",
    "lint": "wix-node-build lint"
  },
  "wix": {
    "entry": {
      "app": "index.js"
    },
    "externals": {
      "react": "React"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/wix-style-react.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix/wix-style-react/issues"
  },
  "homepage": "https://github.com/wix/wix-style-react#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-options": "^1.0.1",
    "@kadira/storybook-deployer": "^1.2.0",
    "autosuggest-highlight": "^3.1.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "*",
    "babel-preset-stage-2": "*",
    "babel-runtime": "^6.6.1",
    "cheerio": "^0.22.0",
    "copy-to-clipboard": "^3.0.5",
    "co": "^4.6.0",
    "enzyme": "^2.3.0",
    "esformatter": "^0.10.0",
    "esformatter-jsx": "^7.4.1",
    "eslint-plugin-react": "^6.8.0",
    "eyes.it": "^1.0.232",
    "gh-pages-auto-release": "^1.1.2",
    "glob": "^7.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jquery": "^1.12.4",
    "jsdom": "^9.2.1",
    "lodash.camelcase": "^4.3.0",
    "ps-tree": "^1.1.0",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-collapse": "^2.3.3",
    "react-dom": "^15.0.0",
    "react-element-to-jsx-string": "^6.0.0",
    "react-height": "^2.1.1",
    "react-motion": "^0.4.7",
    "react-test-utils": "0.0.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "svgo": "^0.7.1",
    "wix-node-build": "^1.1.93"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "github-markdown-css": "^2.4.1",
    "highlight.js": "^9.8.0",
    "jquery": "^1.12.4",
    "lodash.defer": "^4.1.0",
    "lodash.difference": "^4.5.0",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.findindex": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.has": "^4.5.2",
    "lodash.includes": "^4.3.0",
    "lodash.initial": "^4.1.1",
    "lodash.isequal": "^4.4.0",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "lodash.last": "^3.0.0",
    "lodash.map": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.reduce": "^4.6.0",
    "lodash.remove": "^4.7.0",
    "lodash.toarray": "^4.4.0",
    "lodash.trim": "^4.5.1",
    "lodash.uniqueid": "^4.0.1",
    "rc-slider": "^5.3.4",
    "react-addons-css-transition-group": "^15.4.2",
    "react-autosuggest": "^7.0.1",
    "react-datepicker": "^0.39.0",
    "react-modal": "^1.5.2",
    "react-onclickoutside": "^5.7.1",
    "react-remarkable": "^1.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ]
  },
  "eslintConfig": {
    "extends": "wix/react",
    "env": {
      "jest": true
    },
    "rules": {
      "react/jsx-handler-names": 0,
      "react/no-find-dom-node": 0,
      "react/no-string-refs": 0,
      "react/jsx-boolean-value": 0
    }
  }
}
